{
  "name": "nitro-react-autocomplete",
  "version": "0.0.4",
  "description": "Nitro React Autocomplete Search",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server",
    "watch": "jest --coverage --watch",
    "build": "webpack"
  },
  "repository": "git+https://github.com/mrc535s/react-autocomplete.git",
  "keywords": [
    "reactjs",
    "autocomplete",
    "search"
  ],
  "author": "Michael Carney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrc535s/react-autocomplete/issues"
  },
  "homepage": "https://github.com/mrc535s/react-autocomplete#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "expect": "^1.20.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "engines": {
    "node": ">=6.9"
  },
  "engineStrict": true
